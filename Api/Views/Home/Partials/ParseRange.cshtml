@using Models.ViewModels
@model PathInput

@using (Html.BeginForm("ParseRange", "Home", FormMethod.Post))
{
    <div class="form-group">
        @Html.LabelFor(x => x.Path, "Range PDF path")
        @Html.TextBoxFor(x => x.Path, new { @class="form-control", type="text" })
    </div>
    
    <button type="submit" class="btn btn-default">Submit</button>
}
