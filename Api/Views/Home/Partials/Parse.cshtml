@model Models.ViewModels.PathInput

@using (Html.BeginForm("Parse", "Home", FormMethod.Post))
{
    <div class="form-group">
        @Html.LabelFor(x => x.Path, "Exact file path")
        @Html.TextBoxFor(x => x.Path, new { @class="form-control", type="text" })
    </div>
    
    <button type="submit" class="btn btn-default">Submit</button>
}
